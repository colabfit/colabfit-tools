    "### Generating configurations"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "id": "cI3rM1WyLV-2"
   },
   "source": [
    "#### Manually"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "kIgYQ1-4Laqd"
   },
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "from ase import Atoms\n",
    "\n",
    "images = []\n",
    "for i in range(1, 1000):\n",
    "    atoms = Atoms('H'*i, positions=np.random.random((i, 3)))\n",
    "\n",
    "    atoms.info['_name'] = 'configuration_' + str(i)\n",
    "    \n",
    "    atoms.info['dft_energy'] = i*i\n",
    "    atoms.arrays['dft_forces'] = np.random.normal(size=(i, 3))\n",
    "    \n",
    "\n",
    "    images.append(atoms)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {
    "id": "fnGXTsizLWBY"
   },
   "source": [
    "#### Using `load_data()`"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "mZqrOAeWMjCn"
   },
   "outputs": [],
   "source": [
    "from ase.io import write\n",
    "\n",
    "# outfile = '/content/example.extxyz'   # use this line for ColabFit\n",
    "outfile = '/tmp/example.extxyz'   # use this line for local runs\n",
    "\n",
    "write(outfile, images)  # use this line for local runs"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "1aa-2NkBMx_B"
   },
   "outputs": [],
   "source": [
    "from colabfit.tools.database import load_data\n",
    "\n",
    "images = list(load_data(\n",
    "    file_path=outfile,  # use this line for local runs\n",
    "    file_format='xyz',\n",
    "    name_field='_name',\n",
    "    elements=['H'],\n",
    "    default_name=None,\n",
    "    verbose=True\n",
    "))"
   ]
  },
  {
